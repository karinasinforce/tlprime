<script>
    
    document.querySelectorAll('a[href^="#"]').forEach(anchor => {
        anchor.addEventListener('click', function (e) {
            e.preventDefault();

            document.querySelector(this.getAttribute('href')).scrollIntoView({
                behavior: 'smooth'
            });
        });
    });
    
    $(document).ready(function() {
        //Carrossel
        $('.owl-carousel').owlCarousel({
            loop:true,
            nav:true,
            navText: ['', ''],
            responsive:{
                0:{
                    items:1
                },
                600:{
                    items:3
                },
                1000:{
                    items:5
                }
            }
        })

        //Galeria de imagens
        $lg = $('.gallery__wrapper').lightGallery({
            selector: '.gallery__item',
            escKey: true,
            thumbnail:true,
            lazyLoad:true
        });

        $('.open-gallery-emp').click(function() {
            var $items = $('.gallery-emp').find('.gallery__item')
            
            if ($items) {
                var first = $items[0];
                $(first).click();
            }
        });

        $('.open-gallery-lazer').click(function() {
            var $items = $('.gallery-lazer').find('.gallery__item')
            
            if ($items) {
                var first = $items[0];
                $(first).click();
            }
        });

      
         /* Funcao que trata os dados do form de contato para envio para a API */
        $(".form-contato").submit(function(e) {
            debugger
            e.preventDefault();
            
            var $this = $(this); 
            var $btnSubmit = $this.find("button[type=submit]");
            $btnSubmit.attr("disabled", true);
            $btnSubmit.find('.button__text').text('Enviando...')

            var url                 = "{{API_PATH}}/{{API_KEY}}/Formulario/Generico";
            var error               = null;
            var form                = $(this);

            var message             = "";
            var unindexed_array     = form.serializeArray();
            var indexed_array       = IndexDataForm(unindexed_array);
            var values_array        = indexed_array;
            var values              = "";
            
            
            // prepara os inputs com os data=values
            values_array            = IndexDataForm( form.find(":input[data='values']").serializeArray() );
            indexed_array.values    = JSON.stringify(values_array);
           
            // envia para a API
            $.ajax({
                method: "POST",
                url:    url,
                data:   indexed_array
            })
            .done(function( resp ) {
                debugger
                console.log(resp);
                form.find('.contato-mss').html( '<p class="form-wrapper__message">' + resp.Message + '</p>' );
                form.parent('.box-contato').css("opacity","1");
                $btnSubmit.attr("disabled", false);
                $btnSubmit.find('.button__text').text('Enviar');
            })
            .fail(function(resp){
                debugger
                console.log("erro de consumo da api");
                console.log(resp);
                form.find('.contato-mss').html( '<p class="form-wrapper__message">Por favor, tente novamente em alguns minutos.</p>' );
                form.parent('.box-contato').css("opacity","1");
                $btnSubmit.attr("disabled", false);
                $btnSubmit.find('.button__text').text('Enviar');
            });

            return false;
        });    

        function IndexDataForm(unindexed_array){
            var indexed_array   = {};
            var nKey            = "";
            var nVal            = "";
            
            $.map(unindexed_array, function(n, i){
                iVal = n['value'];
                if(iVal){
                    
                    iVal = iVal.split('.').join("");
                    if( (n['name']=='email') || (n['name']=='Email') ){
                        iVal = n['value'];
                    }
                        
                    iVal = iVal.replace( /\+/g, '');
                    if(nKey==n['name']){
                        nVal = nVal+";"+iVal;

                    }else{
                        nVal = iVal;
                    }            
                    nKey = n['name'];
                    indexed_array[nKey] = nVal;
                }
            });
            
            return indexed_array;
        }


        {% if (content.MapaLatitude and content.MapaLongitude) or content.Endereco %}
             var activateMap = (function() {
                
                try{

                    var geocoder = new google.maps.Geocoder();

                    var address;    
                    {% if item.produtotag=='lancamentos' %}
                        {% if item.MapaLatitude|default("") and item.MapaLongitude|default("") %}
                            address = "{{content.MapaLatitude}}, {{content.MapaLongitude}}";
                        {% else %}
                            address = "Brasil, {{content.CidadeNome}}, {{content.BairroNome}} - {{content.Localizacao}}";
                        {% endif %}
                    {% else %}
                        address = "{{content.EnderecoLogradouro}} - {{content.BairroNome}}, {{content.CidadeNome}} {{content.EstadoSigla}}, Brasil  ";
                    {% endif %}


                    var latitude = "";
                    var longitude = ""; 

                    geocoder.geocode( {'address':address}, function(results) {
                        latitude =  latitude + results[0].geometry.location.lat();
                        longitude = longitude + results[0].geometry.location.lng();

                        var latlng = new google.maps.LatLng(latitude, longitude);
                        var myOptions = {
                            zoom: 14,
                            center: latlng,
                            scrollwheel: false,
                            draggable: false,
                            mapTypeId: google.maps.MapTypeId.ROADMAP
                        };

                        var map = new google.maps.Map(document.getElementById("map_canvas"), myOptions);

                        new google.maps.Circle({
                            strokeColor: '#ffc74b',
                            strokeOpacity: 0.8,
                            strokeWeight: 2,
                            fillColor: '#ffc74b',
                            fillOpacity: 0.35,
                            map: map,
                            center: latlng,
                            radius: 500
                        });
                        
                        {# 
                        
                        var image = '/img/icon_placemark_sm2.png';
                        var imageHover = '/img/icon_placemark_sm2.png';
                        var myLatLng = new google.maps.LatLng(latitude, longitude);
                        
                        var marker = new google.maps.Marker({
                            position: myLatLng,
                            map: map,
                            icon: image
                        });

                        var contentString = '<h6>'+address+'</h6>';
                        var infowindow = new google.maps.InfoWindow({ 
                            content: contentString,
                            maxWidth: 290
                        });

                        google.maps.event.addListener(marker, 'click', function() {
                            infowindow.open(map,marker);
                        }); #}

                        //google.maps.event.addListener(marker, 'mouseout', function() {
                            //infowindow.close(map,marker);
                        //});

                        // Hover
                        //google.maps.event.addListener(marker, 'click', function() {
                            //this.setIcon(imageHover);
                        //});

                        //google.maps.event.addListener(marker, 'mouseout', function() {
                            //this.setIcon(image);
                        //});
                    });
                    
                }catch(err){
                    console.log(err.message);
                }
                
            });

            $('#activate-map').click(function() {
                activateMap();
                $('.container_button_map').hide();
                $('#map_canvas').removeClass('blur');
            });
        {% endif %}
    })
    
</script>

