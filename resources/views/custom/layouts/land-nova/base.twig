{% spaceless %}

{% if session_get('parametros') %}{% set siteparam = session_get('parametros') %}{% else %}{% set siteparam = '' %}{% endif %}
{% if session_get('corretor') %}{% set sitecorretor = session_get('corretor') %}{% else %}{% set sitecorretor = '' %}{% endif %}

{% set title = "Enjoy Brasil Brokers " ~ content.Nome|default('') ~ ', ' ~ content.BairroNome|default('') ~ ' - ' ~ content.CidadeNome|default('')  %}
{% set description = "Empreendimento" ~ ( content.BairroNome|default('') != '' ? ' situado em ' ~ content.BairroNome ) ~ ( content.Localizacao|default('') != '' ? ' (' ~ content.Localizacao ~ ')' : '') ~ ' - ' ~ content.CidadeNome  ~ " " %}


<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, user-scalable=no" />
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>{{title}} | {{APP_NAME}}</title>
    <meta name="description" content="{{description}}">

    <meta property="og:url" content="{{url_current()}}" />
    <meta property="og:type" content="website" />
    <meta property="og:title" content="Enjoy Brasil Brokers - {{content.Nome}}" />
    <meta property="og:description" content="{{item.Descricao}}" />
    
    
    {% if content.Topicos[0].Imagens[0] is defined %}
        <meta property="og:image" content="{{IMG_PATH}}/{{content.Topicos[0].Imagens[0]}}.jpg?mw=300&wm=5" />
    {% else %}
        <meta property="og:image" content="{{IMG_PATH}}/img/facebook-share.png" /> 
    {% endif %}

    {% if content.UrlTrack is defined %}
        <link rel="canonical" href="{{SITE_URL}}lancamento/{{content.UrlTrack}}/ID-{{content.ID}}"/>
    {% endif %}

    <link rel="stylesheet" href="/css/bootstrap.min.css">
    <link href="https://fonts.googleapis.com/css?family=Montserrat:300,400,700,700i,800,800i&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.11.2/css/all.min.css">
    <link rel="stylesheet" href="/js/plugins/owl.carousel.min.css">
    <link rel="stylesheet" href="/css/land-nova.css">
    <link href="/js/plugins/lightGallery/css/lightgallery.min.css" rel="stylesheet">    

    <!-- Google Maps -->
    <script type="text/javascript" defer src="https://maps.googleapis.com/maps/api/js?key={{siteparam.googlemapskey}}"></script>
 
    {% if siteparam.googleanalytics %}
        <script>
            var _gacode = '{{siteparam.googleanalytics}}';

            (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
            (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
            m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
            })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');

            ga('create', _gacode, 'auto');
            ga('send', 'pageview');

        </script>        
    {% endif %}
    
    {% if siteparam.facebookpixel %}
        <!-- Facebook Pixel Code -->
        <script>
            !function(f,b,e,v,n,t,s)
            {if(f.fbq)return;n=f.fbq=function(){n.callMethod?
            n.callMethod.apply(n,arguments):n.queue.push(arguments)};
            if(!f._fbq)f._fbq=n;n.push=n;n.loaded=!0;n.version='2.0';
            n.queue=[];t=b.createElement(e);t.async=!0;
            t.src=v;s=b.getElementsByTagName(e)[0];
            s.parentNode.insertBefore(t,s)}(window,document,'script',
            'https://connect.facebook.net/en_US/fbevents.js');

            fbq('init', '{{siteparam.facebookpixel}}'); 
            fbq('track', 'PageView');
        </script>

        <noscript>
            <img height="1" width="1" src="https://www.facebook.com/tr?id=1863129267342172&ev=PageView&noscript=1"/>
        </noscript>
        <!-- End Facebook Pixel Code --> 
    {% endif %}        
    
</head>
<body>
    <div class="wrapper-content">
        <header class="header">
            <!-- Menu Desktop-->
            <div class="container d-none d-md-flex justify-content-end">
                <div class="row w-100">
                    <div class="col-2 d-flex justify-content-center align-items-center">
                        <div class="logo" style="background-image: url('/img/logo_landing_page.png');"></div>
                    </div>
                    <div class="col-7 d-flex  align-items-center">
                        <div class="dropdown">
                            <button class="btn btn-secondary dropdown-toggle menu-dropdown-nav" type="button" id="dropdown-menu" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                Navegação
                            </button>
                            <div class="dropdown-menu" aria-labelledby="dropdown-menu">
                                {% for item in content.Topicos %}
                                    <a class="dropdown-item" href="#{{item.Nome|replace({' ':'-', '.':''})|lower}}">{{item.Nome}}</a>
                                {% endfor %}
                            </div>
                        </div>
                    </div>
                    <div class="col-3">
                        <div class="contact-buttons">
                            <a class="contact-buttons__button contact-buttons__info-mail button button--effect1"
                                href="#bloco-6">
                                <i class="far fa-envelope fa-2x"></i>
                                <p class="contact-buttons__title button__text button__text--header">
                                    Informações via Email
                                </p >
                            </a>
                            <a class="contact-buttons__button contact-buttons__info-whatsapp button button--effect1"
                                href="https://api.whatsapp.com/send?phone={{siteparam.telefones.whatsappnumber}}&text=Olá, gostaria de mais informações sobre o empreendimento {{content.Nome}}." target="_blank">
                                <i class="fab fa-whatsapp fa-2x"></i>
                                <p class="contact-buttons__title button__text button__text--header">
                                    Informações via Whatsapp
                                </p >
                            </a>
                        </div>
                    </div>
                </div>
            </div>
            <!-- Menu Desktop-->
            
            <!-- Menu Mobile -->
            <nav role="navigation" class="d-md-none">
                <div class="row w-100">
                    <div class="col-6">
                        <div id="menuToggle">
                            <input type="checkbox" />
                            <span></span>
                            <span></span>
                            <span></span>
                            <ul id="menu">
                                {% for item in content.Topicos %}
                                    <li><a href="#{{item.Nome|replace({' ':'-', '.':''})|lower}}">{{item.Nome}}</a></li>
                                {% endfor %}
                            </ul>
                        </div>
                    </div>
                    <div class="col-6 d-flex justify-content-end align-items-center">
                        <div class="logo" style="background-image: url('/img/logo_landing_page.png');"></div>
                    </div>
                </div>
            </nav>
            <!-- Menu Mobile -->
        </header>

        <section class="banner" style="background-image: url('{{data.APP_ADMIN|replace({'http://':'https://'})}}/api/empreendimento/capa/{{content.ID}}?v=1');">
            <div class="container">
                <div class="col-md-5 col-12 offset-md-7 d-flex justify-content-center justify-content-md-end p-0">
                    <div class="box-actions-wrapper">
                        <div class="box-actions">
                            <div class="box-actions__price">
                                <p class="box-actions__price-text">a partir de</p>
                                <p class="box-actions__price-number">{{content.ValorMenor ? 'R$ ' ~ content.ValorMenor|number_format(0,',', '.') : '-'}}</p>
                            </div>
        
                            <h1 class="box-actions__title">{{content.Nome}}</h1>
                            <h2 class="box-actions__subtitle box-actions__subtitle--with-border">{{content.BairroNome}} - {{content.CidadeNome}}</h2>
        
                            <div class="bloco__info-icons">
                                <div class="row">
                                    <div class="col-4 bloco__info-icons__item bloco__info-icons__item--small-text bloco__info-icons__item--auto">
                                        <i class="fa fa-home fa-2x"></i>
                                        <span>{{content.NaturezaNome}}</span>
                                    </div>
                                    <div class="col-4 bloco__info-icons__item bloco__info-icons__item--small-text bloco__info-icons__item--auto">
                                        <i class="fas fa-bed fa-2x"></i>
                                        <span>{{content.QtdQuarto ? content.QtdQuarto|join(', ') ~ ' quarto(s)' : '-'}} </span>
                                    </div>
                                    <div class="col-4 bloco__info-icons__item bloco__info-icons__item--small-text bloco__info-icons__item--auto">
                                        <i class="fas fa-vector-square fa-2x"></i>
                                        <span>
                                            de 
                                            {{content.M2Menor ? content.M2Menor ~ 'm²' : '-'}}
                                            até 
                                            {{content.M2Maoir ? content.M2Maoir ~ 'm²' : '-'}}
                                        </span>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="contact-buttons">
                            <a class="contact-buttons__button contact-buttons__button--no-direction contact-buttons__info-mail button button--effect1"
                                href="#bloco-6">
                                <i class="far fa-envelope fa-2x"></i>
                                <span class="contact-buttons__title contact-buttons__title--medium">
                                    Informações via Email
                                </span>
                            </a>
                            <a class="contact-buttons__button contact-buttons__button--no-direction contact-buttons__info-whatsapp button button--effect1"
                                href="https://api.whatsapp.com/send?phone={{siteparam.telefones.whatsappnumber}}&text=Olá, gostaria de mais informações sobre o empreendimento {{content.Nome}}." target="_blank">
                                <i class="fab fa-whatsapp fa-2x"></i>
                                <span class="contact-buttons__title contact-buttons__title--medium">
                                    Informações via Whatsapp
                                </span>
                            </a>
                        </div>
                        <div class="button button--effect1 button--full button--strong button--center button--font-medium">
                            <a class="button__text button__text--white" href="#o-empreendimento">Quero conhecer o empreendimento</a>
                        </div>
                    </div>
                </div>
            </div>
        </section>
        
        {% if content.Topicos %}
            {% for key, item in content.Topicos %}
                
                {% if item.Nome == 'O empreendimento' %}
                    <section id="{{item.Nome|replace({' ':'-', '.':''})|lower}}" class="container bloco">
                        <div class="row">
                            <div class="col-12 col-md-5">
                                {% if item.Imagens %}
                                    <div class="image-capa-left open-gallery-emp" style="background-image: url('{{data.IMG_PATH}}/{{item.Imagens[0]}}.jpg?wm=5');"></div>
                                {% else %}
                                    <div class="image-capa-left open-gallery-emp"  style="background-image: url('{{data.IMG_PATH}}/{{content.ImgVitrineID}}.jpg?wm=5');"></div>
                                {% endif %}
                            </div>
                            <div class="col-12 col-md-7">
                                <div class="bloco__info">
                                    <h2 class="title">
                                        {{item.Nome}}
                                        <hr class="title__line title__line--left">
                                    </h2>
                                    <div class="text-body">
                                        {{item.Descricao|raw}}
                                    </div>

                                    <div class="bloco__info-icons">
                                        <div class="row">
                                            <div class="col-4 bloco__info-icons__item">
                                                <i class="fa fa-home fa-3x"></i>
                                                <span>{{content.NaturezaNome|default('-')}}</span>
                                            </div>
                                            <div class="col-4 bloco__info-icons__item">
                                                <i class="fas fa-tools fa-3x"></i>
                                                <span>{{content.StatusNome|default('-')}}</span>
                                            </div>
                                            <div class="col-4 bloco__info-icons__item">
                                                <i class="fa fa-bed fa-3x"></i>
                                                <span>{{content.QtdQuarto ? content.QtdQuarto|join(', ') ~ ' quarto(s)' : '-'}}</span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="gallery d-none gallery-emp">
                            <div class="gallery__wrapper">
                                {% for img in item.Imagens %}
                                    <div class="gallery__item" data-src="{{IMG_PATH}}/{{img}}.jpg?wm=5">
                                        <img src="{{IMG_PATH}}/thumb/{{img}}.jpg?wm=5"/>
                                    </div>
                                {% endfor %}
                            </div>
                        </div>
                    </section>
                {% endif %}

                {% if item.Nome == 'Lazer' %}
                    <section id="{{item.Nome|replace({' ':'-', '.':''})|lower}}" class="bloco">
                        <div class="container d-flex justify-content-center align-items-center">
                            <a class="title-custom title-custom--seta" href="#lazer">
                                Conheça os itens de lazer
                            </a>
                        </div>

                        <div class="float-bar-actions float-bar-actions--top-space">
                            <div class="container">
                                <div class="row">
                                    <div class="col-8 pl-0">
                                        <div class="float-bar-actions__title-box">
                                            <h3 class="float-bar-actions__title-box__title">Quer mais informações sobre esse empreendimento ?</h3>
                                            <p class="float-bar-actions__title-box__subtitle">Ficaremos extremamente felizes em realizar este contato</p>
                                        </div>
                                    </div>
                                    <div class="col-4">
                                        <div class="contact-buttons">
                                            <a class="contact-buttons__button contact-buttons__info-mail button button--effect1"
                                                href="#bloco-6">
                                                <i class="far fa-envelope fa-3x"></i>
                                                <p class="contact-buttons__title contact-buttons__title--big button__text">
                                                    Informações via Email
                                                </p >
                                            </a>
                                            <a class="contact-buttons__button contact-buttons__info-whatsapp button button--effect1"
                                                href="https://api.whatsapp.com/send?phone={{siteparam.telefones.whatsappnumber}}&text=Olá, gostaria de mais informações sobre o empreendimento {{content.Nome}}." target="_blank">
                                                <i class="fab fa-whatsapp fa-3x"></i>
                                                <p class="contact-buttons__title contact-buttons__title--big button__text">
                                                    Informações via Whatsapp
                                                </p >
                                            </a>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="container-fluid">
                            <div class="d-flex flex-column flex-md-row">
                                <div class="col-12 col-md-4 order-2 order-md-1 offset-md-2">
                                    <div class="bloco__info ajust-container">
                                        <h2 class="title">
                                            {{item.Nome}}
                                            <hr class="title__line title__line--left">
                                        </h2>
                                        <div class="text-body">
                                            {{item.Descricao|raw}}
                                        </div>
                                    </div>
                                </div>
                                <div class="col-12 col-md-6 order-1 order-md-2 pr-0">
                                    {% if item.Imagens %}
                                        <div class="image-capa-left open-gallery-lazer" style="background-image: url('{{data.IMG_PATH}}/{{item.Imagens[0]}}.jpg?wm=5');"></div>
                                    {% else %}
                                        <div class="image-capa-left open-gallery-lazer" style="background-image: url('{{data.IMG_PATH}}/{{content.ImgVitrineID}}.jpg?wm=5');"></div>
                                    {% endif %}
                                </div>
                            </div>
                        </div>

                        <div class="float-bar-actions">
                            <div class="container">
                                <div class="row">
                                    <div class="col-8 pl-0">
                                        <div class="float-bar-actions__title-box">
                                            <h3 class="float-bar-actions__title-box__title">Quer mais informações sobre esse empreendimento ?</h3>
                                            <p class="float-bar-actions__title-box__subtitle">Ficaremos extremamente felizes em realizar este contato</p>
                                        </div>
                                    </div>
                                    <div class="col-4">
                                        <div class="contact-buttons">
                                            <a class="contact-buttons__button contact-buttons__info-mail button button--effect1"
                                                href="#bloco-6">
                                                <i class="far fa-envelope fa-3x"></i>
                                                <p class="contact-buttons__title contact-buttons__title--big button__text">
                                                    Informações via Email
                                                </p >
                                            </a>
                                            <a class="contact-buttons__button contact-buttons__info-whatsapp button button--effect1"
                                                href="https://api.whatsapp.com/send?phone={{siteparam.telefones.whatsappnumber}}&text=Olá, gostaria de mais informações sobre o empreendimento {{content.Nome}}." target="_blank">
                                                <i class="fab fa-whatsapp fa-3x"></i>
                                                <p class="contact-buttons__title contact-buttons__title--big button__text">
                                                    Informações via Whatsapp
                                                </p >
                                            </a>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="gallery d-none gallery-lazer">
                            <div class="gallery__wrapper">
                                {% for img in item.Imagens %}
                                    <div class="gallery__item" data-src="{{IMG_PATH}}/{{img}}.jpg?wm=5">
                                        <img src="{{IMG_PATH}}/thumb/{{img}}.jpg?wm=5"/>
                                    </div>
                                {% endfor %}
                            </div>
                        </div>
                    </section>
                {% endif %}

                {% if item.Nome != 'Lazer' and item.Nome != 'O empreendimento' and item.Nome != 'Vídeo' and item.Nome != 'Localização' %}
                    <section id="{{item.Nome|replace({' ':'-', '.':''})|lower}}" class="bloco">
                        <div class="container">
                            <div class="bloco__info full">
                                <h2 class="title title--center">
                                    {{item.Nome}}
                                    <hr class="title__line">
                                </h2>
                                <div class="text-body">
                                    {{item.Descricao|raw}}
                                </div>
                            </div>
                        </div>
                        {% if item.Imagens %}
                            <div class="gallery">
                                <div class="owl-carousel owl-theme gallery__wrapper">
                                    {% for img in item.Imagens %}
                                        <div class="gallery__item" 
                                            style="background-image: url('{{IMG_PATH}}/{{img}}.jpg?mw=500&wm=5');" 
                                            data-src="{{IMG_PATH}}/{{img}}.jpg?wm=5">
                                            
                                            <img src="{{IMG_PATH}}/thumb/{{img}}.jpg?wm=5"/>
                                        </div>
                                    {% endfor %}
                                </div>
                            </div>
                        {% endif %}
                        <div class="float-bar-actions">
                            <div class="container">
                                <div class="row">
                                    <div class="col-8 pl-0">
                                        <div class="float-bar-actions__title-box">
                                            <h3 class="float-bar-actions__title-box__title">Quer mais informações sobre esse empreendimento ?</h3>
                                            <p class="float-bar-actions__title-box__subtitle">Ficaremos extremamente felizes em realizar este contato</p>
                                        </div>
                                    </div>
                                    <div class="col-4">
                                        <div class="contact-buttons">
                                            <a class="contact-buttons__button contact-buttons__info-mail button button--effect1"
                                                href="#bloco-6">
                                                <i class="far fa-envelope fa-3x"></i>
                                                <p class="contact-buttons__title contact-buttons__title--big button__text">
                                                    Informações via Email
                                                </p >
                                            </a>
                                            <a class="contact-buttons__button contact-buttons__info-whatsapp button button--effect1"
                                                href="https://api.whatsapp.com/send?phone={{siteparam.telefones.whatsappnumber}}&text=Olá, gostaria de mais informações sobre o empreendimento {{content.Nome}}." target="_blank">
                                                <i class="fab fa-whatsapp fa-3x"></i>
                                                <p class="contact-buttons__title contact-buttons__title--big button__text">
                                                    Informações via Whatsapp
                                                </p >
                                            </a>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </section>
                {% endif %}
                
                {% if item.Nome == 'Video' %}
                    <section id="bloco-5" class="bloco bloco--no-bottom">
                        <div class="container">
                            <div class="bloco__info full">
                                <h2 class="title title--center">
                                    Vídeo
                                    <hr class="title__line">
                                </h2>
                            </div>
                        </div>
                        
                        <div class="container">
                            <div class="row">
                                <iframe width="100%" height="500" src="{{item.Descricao}}" frameborder="0" allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
                            </div>
                        </div>
                    </section>
                {% endif %}
            {% endfor %}

            {% if (content.MapaLatitude and content.MapaLongitude) or content.Endereco %}
                <section id="localização" class="bloco bloco--no-bottom">
                    <div class="container">
                        <div class="bloco__info full">
                            <h2 class="title title--center">
                                Localização
                                <hr class="title__line">
                            </h2>
                            <div class="text-body">
                                {{content.Endereco}}
                            </div>
                        </div>
                    </div>
                </section>
                <div class="row-box maps stack">
                    <div id="map_canvas" class="blur">
                    </div>
                    <div class="container_button_map" id="activate-map">
                        <button class="btn btn-primary">Exibir Mapa</button>
                    </div>
                </div>
            {% endif %}
        {% endif %}

        <section id="bloco-6" class="bloco__form">
            <div class="container">
            <div class="row">
                <div class="d-none d-md-block col-12 col-md-5">
                        <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" version="1.1" id="Camada_1" x="0px" y="0px" width="350px" viewBox="0 0 361.1 320" style="enable-background:new 0 0 361.1 320;" xml:space="preserve" class="img-responsive tosvg replaced-svg">
                            <style type="text/css">
                                .st0{fill:#FFFFFF;}
                            </style>
