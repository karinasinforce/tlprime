    <meta name="API_PATH" id="API_PATH" data-path="{{API_PATH}}"/>

    <title>{{ html_header_title|raw }} {{ actual_page != 'page-home' ? '| ' ~ APP_NAME : '' }}</title>    
      
    {# facebook og #}
        {% if header_facebook_url  %}
            <meta property="og:url" content="{{header_facebook_url}}" />
        {% endif %}
